repeat wait() until game:IsLoaded()

local VirtualUser = game:GetService("VirtualUser")

game:GetService("Players").LocalPlayer.Idled:Connect(
    function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end
)

local nameOfGame = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)

local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/UI-Interface/CustomFIeld/main/RayField.lua'))()

local Window = Rayfield:CreateWindow({
    Name = nameOfGame.Name,
    LoadingTitle = "I LOVE BOOBIES HUB",
    LoadingSubtitle = "by ThatOneGuy",
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil, 
       FileName = "(.)(.) Hub"
    }
 })

 local Tab = Window:CreateTab("I'm OP NOW")

 local Toggle = Tab:CreateToggle({
    Name = "Auto Tap",
    CurrentValue = false,
    Flag = "TapToggle1", 
    Callback = function(Value)
     autotap = Value
     if autotap then
         pcall(function ()
             while autotap do
                 for i=1, 10 do
                 task.wait()
                    game:GetService("ReplicatedStorage").Remotes.attack:FireServer()
                 end
             end
         end)
     end
 end,
 })

 local Toggle = Tab:CreateToggle({
    Name = "Auto Skill",
    CurrentValue = false,
    Flag = "SkillToggle1", 
    Callback = function(Value)
     autoSkill = Value
     if autoSkill then
         pcall(function ()
             while autoSkill do
                local args = {
                    [1] = "Slot1"
                }

                game:GetService("ReplicatedStorage").Remotes:FindFirstChild("Send Information"):FireServer(unpack(args))
                wait()           
             end
         end)
     end
 end,
 })

 local Toggle = Tab:CreateToggle({
    Name = "Auto Collect",
    CurrentValue = false,
    Flag = "CollectToggle1", 
    Callback = function(Value)
     autoCollect = Value
     if autoCollect then
         pcall(function ()
             while autoCollect and task.wait() do
                local plr = game.Players.LocalPlayer.Character
                local Coin = game:GetService("Workspace").Coins:GetChildren()
                for i, v in pairs(Coin) do
                    if v:IsA("MeshPart") then
                        v.CFrame = plr.HumanoidRootPart.CFrame
                    end
                end
             end
         end)
     end
 end,
 })

 local worlds = {}

 for _, k in pairs(game:GetService("ReplicatedStorage")["Unloaded_Maps"]:GetChildren()) do
    if k:IsA("Folder") then
    table.insert(worlds,k.Name)
        for _, o in pairs(game:GetService("Workspace").EnemyNPCs:GetChildren()) do
            if o:IsA("Folder") then
            table.insert(worlds,o.Name)
            end
        end
    end
end


local Dropdown = Tab:CreateDropdown({
    Name = "Select World",
    Options = worlds,
    CurrentOption = "",
    Flag = "worldfolder",
    Callback = function(Option)
        PickWorld = Option
    end,
 })

 local Dropdown = Tab:CreateDropdown({
    Name = "Select Mob",
    Options = {"Weak", "Strong", "Miniboss", "Boss", "RaidBoss"},
    CurrentOption = "",
    Flag = "MobNum1",
    Callback = function(Option)
        Mob = Option
    end,
 })

 local Toggle = Tab:CreateToggle({
    Name = "Auto Farm",
    CurrentValue = false,
    Flag = "FarmToggle1", 
    Callback = function(Value)
     autoFarm = Value
     if autoFarm then
         pcall(function ()
             while autoFarm and task.wait() do
                        local plr = game.Players.LocalPlayer.Character
                        for _, v in pairs(game:GetService("Workspace").EnemyNPCs[PickWorld]:GetChildren()) do
                        if v.Name == Mob then
                            for _, l in pairs(v:GetChildren()) do
                            if l ~= nil then
                                plr.HumanoidRootPart.CFrame = l.HumanoidRootPart.CFrame * CFrame.new (0,0,2)
                                end
                            end
                        end
                end
             end
         end)
     end
 end,
 })
